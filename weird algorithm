#include <bits/stdc++.h>
using namespace std;

void print(int n){
    
    if(n==1){
        cout<<n<<"  ended"<<endl;
        return;
    }
    cout<<n<<"-> ";
    if(n%2==0)
      print(n/2);
    else
      print(3*n+1);
}




int main() {
    
    int n;
    cin>>n;
    
    print(n);

    return 0;
}
